&constants
/

&model
  model_type = 'EVOL'  ! Obtain stellar structure from an evolutionary model
  file = './LOGS/profile39.data.GYRE'    ! File name of the evolutionary model
  file_format = 'MESA' ! File format of the evolutionary model
/

&mode
  l = 1 ! Harmonic degree
  n_pg_min = -200
  n_pg_max=-1
  tag = 'lo'
/
&mode
  l = 2 ! Harmonic degree
  n_pg_min = -200
  n_pg_max=-1
  tag = 'lo'
/
&mode
  l = 3 ! Harmonic degree
  n_pg_min = -200
  n_pg_max=-1
  tag = 'hi'
/
&mode
  l = 4 ! Harmonic degree
  n_pg_min = -220
  n_pg_max=-1
  tag = 'hi'
/
&mode
  l = 5 ! Harmonic degree
  n_pg_min = -250
  n_pg_max=-1
  tag = 'hi'
/

&osc
    nonadiabatic = .TRUE.
/

&rot
/

&num
  diff_scheme = 'MAGNUS_GL2' 
!  diff_scheme = 'COLLOC_GL2' 
  n_iter_max = 200
/

&scan
  grid_type = 'INVERSE' ! Scan grid uniform in inverse frequency
  freq_min_units = 'UHZ'
  freq_max_units = 'UHZ'
  freq_min = 2           ! Minimum frequency to scan from
  freq_max = 200         ! Maximum frequency to scan to
  n_freq = 5000          ! Number of frequency points in scan
  tag_list = 'hi'
/

&scan
  grid_type = 'INVERSE' ! Scan grid uniform in inverse frequency
  freq_min_units = 'UHZ'
  freq_max_units = 'UHZ'
  freq_min = 0.4           ! Minimum frequency to scan from
  freq_max = 200         ! Maximum frequency to scan to
  n_freq = 5000          ! Number of frequency points in scan
  tag_list = 'lo'
/

&grid
    w_osc = 20
    w_exp = 4
    w_ctr = 5
/


&ad_output
/

&nad_output
  summary_file_format = 'TXT'
  summary_file = './gyre_output/summary_ell01-05.txt'                         ! File name for summary file
  summary_item_list = 'l,n_pg,freq,freq_units,E_norm' ! Items to appear in summary file
  detail_file_format = 'TXT'
  detail_template = 'gyre_output/mode_ell%L_m%M_n%N.txt'
  detail_item_list = 'j,l,n_pg,n_p,n_g,eta,omega,freq,x,x_ref,f_T,xi_r,xi_r_ref,xi_h,xi_h_ref,lag_L,lag_L_ref,dW_dx,dE_dx,H,L_star,R_star,M_star,T,M_r,rho,P,prop_type,V_2,As,c_1,Gamma_1'
  freq_units = 'UHZ'                   	      ! Units of freq output items
/

